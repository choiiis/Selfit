<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.selfitjava2">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainChat"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ChatRoom"/>
        <activity android:name=".StartSelectActivity"/>
        <activity android:name=".StartManualActivity"/>
        <activity android:name=".StartWorkoutActivity"/>
        <activity android:name=".StartFeedbackActivity"/>
        <activity android:name=".StartBRWorkoutActivity"/>
        <activity android:name=".StartBRManualActivity"/>
        <activity android:name=".StartBRFeedbackActivity"/>
        <activity android:name=".ReviewActivity"/>
        <activity android:name=".GoalMonthActivity"/>
        <activity android:name=".GoalDayActivity"/>

    </application>

</manifest>